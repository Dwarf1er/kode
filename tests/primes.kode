SET MAX TO 100.

SHOW 1.
SHOW 2.

SET X TO 3.
WHILE X LESS THAN MAX OR X EQUALS MAX DO
    SET Y TO 2.

    WHILE Y LESS THAN X OR Y EQUALS X DO
        IF X EQUALS Y THEN
            SHOW X.
        END

        IF X MOD Y EQUALS 0 THEN
            SET Y TO X.
        END

        SET Y TO Y PLUS 1.
    END

    SET X TO X PLUS 1.
END
